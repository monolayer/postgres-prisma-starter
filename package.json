{
	"name": "postgres-prisma-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"prebuild": "npx prisma generate",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"db:bootstrap": "npm run db:migrate && npm run db:seed",
		"db:seed": "npx tsx ./lib/db/seed.ts",
		"db:migrate": "tsx lib/db/migrate.ts",
		"db:studio": "dotenv -f .env.local prisma studio"
	},
	"dependencies": {
		"@monolayer/sdk": "^1.0.1",
		"@prisma/client": "^6.8.2",
		"lorem-ipsum": "^2.0.8",
		"lucide-react": "^0.511.0",
		"next": "^15.5.6",
		"pg": "^8.16.0",
		"prisma": "^6.8.2",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tsx": "^4.19.4"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"dotenv": "^16.5.0",
		"eslint": "^9",
		"eslint-config-next": "^15.5.6",
		"prettier": "^3.5.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"tailwindcss": "^4",
		"typescript": "^5"
	}
}
